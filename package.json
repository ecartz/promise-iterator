{
  "name": "promise-iterator",
  "version": "0.1.0",
  "description": "Async iterator that yields promise results in completion order",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "promise",
    "iterator",
    "async",
    "race",
    "completion",
    "stream"
  ],
  "author": "Matt Fletcher",
  "license": "LGPL-3.0",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
